<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Drawing.Design" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1FlexGrid" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1GanttView" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1InputPanel" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1Schedule" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1SplitContainer" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1SuperTooltip" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.C1Themes" Version="4.8.20231.*" />
    <PackageReference Include="C1.Win.PrintPreview" Version="4.8.20231.*" />
    <PackageReference Include="C1.Zip" Version="2.0.20202.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="AppMenu\AppMenuButton.Designer.cs">
      <DependentUpon>AppMenuButton.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenu\AppMenuItem.Designer.cs">
      <DependentUpon>AppMenuItem.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenu\FileAppMenu.Designer.cs">
      <DependentUpon>FileAppMenu.cs</DependentUpon>
    </Compile>
    <Compile Update="SoftwareDevelopmentPlan.Designer.cs">
      <DependentUpon>SoftwareDevelopmentPlan.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="AppMenu\FileAppMenu.resx">
      <DependentUpon>FileAppMenu.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenu\AppMenuButton.resx">
      <DependentUpon>AppMenuButton.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenu\AppMenuItem.resx">
      <DependentUpon>AppMenuItem.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="SoftwareDevelopmentPlan.resx">
      <DependentUpon>SoftwareDevelopmentPlan.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="ganttview.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
