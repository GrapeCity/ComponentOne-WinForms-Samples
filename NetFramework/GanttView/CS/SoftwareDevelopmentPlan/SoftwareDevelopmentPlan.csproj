<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Drawing.Design" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexGrid" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.GanttView" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.InputPanel" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Ribbon" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Schedule" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.SplitContainer" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.SuperTooltip" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Themes" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Printing" Version="10.0.20252.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="AppMenu\AppMenuButton.Designer.cs">
      <DependentUpon>AppMenuButton.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenu\AppMenuItem.Designer.cs">
      <DependentUpon>AppMenuItem.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenu\FileAppMenu.Designer.cs">
      <DependentUpon>FileAppMenu.cs</DependentUpon>
    </Compile>
    <Compile Update="SoftwareDevelopmentPlan.Designer.cs">
      <DependentUpon>SoftwareDevelopmentPlan.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include=".gclicx" />
    <EmbeddedResource Update="AppMenu\FileAppMenu.resx">
      <DependentUpon>FileAppMenu.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenu\AppMenuButton.resx">
      <DependentUpon>AppMenuButton.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenu\AppMenuItem.resx">
      <DependentUpon>AppMenuItem.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="SoftwareDevelopmentPlan.resx">
      <DependentUpon>SoftwareDevelopmentPlan.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="ganttview.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
