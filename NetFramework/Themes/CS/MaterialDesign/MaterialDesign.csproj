<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.C1Excel" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1Command" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1DX" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1FlexGrid" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1Input" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.Ribbon" Version="8.0.20241.*" />
    <PackageReference Include="C1.Win.C1Schedule" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1SplitContainer" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1Themes" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.Calendar" Version="8.0.20241.*" />
    <PackageReference Include="C1.Win.DashboardLayout" Version="8.0.20241.*" />
    <PackageReference Include="C1.Win.FlexChart" Version="8.0.20241.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\AppRibbonPanel.Designer.cs">
      <DependentUpon>AppRibbonPanel.cs</DependentUpon>
    </Compile>
    <Compile Update="Controls\FormDemo.Designer.cs">
      <DependentUpon>FormDemo.cs</DependentUpon>
    </Compile>
    <Compile Update="Controls\About.Designer.cs">
      <DependentUpon>About.cs</DependentUpon>
    </Compile>
    <Compile Update="Controls\AppThemePanel.Designer.cs">
      <DependentUpon>AppThemePanel.cs</DependentUpon>
    </Compile>
    <Compile Update="Controls\Schedule.Designer.cs">
      <DependentUpon>Schedule.cs</DependentUpon>
    </Compile>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Controls\AppRibbonPanel.resx">
      <DependentUpon>AppRibbonPanel.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\AppThemePanel.resx">
      <DependentUpon>AppThemePanel.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\FormDemo.resx">
      <DependentUpon>FormDemo.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\About.resx">
      <DependentUpon>About.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\Schedule.resx">
      <DependentUpon>Schedule.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\About.png" />
    <EmbeddedResource Include="Resources\Settings.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
