<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="WindowsBase" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.DataSource" Version="4.8.20232.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="GUI\AddIssue.Designer.cs">
      <DependentUpon>AddIssue.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\AssignedIssues.Designer.cs">
      <DependentUpon>AssignedIssues.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\AssignedIssues2.Designer.cs">
      <DependentUpon>AssignedIssues2.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\BatchProcessing.Designer.cs">
      <DependentUpon>BatchProcessing.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\EmployeesIssues.Designer.cs">
      <DependentUpon>EmployeesIssues.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\ReassignFeatures.Designer.cs">
      <DependentUpon>ReassignFeatures.cs</DependentUpon>
    </Compile>
    <Compile Update="GUI\ReassignIssue.Designer.cs">
      <DependentUpon>ReassignIssue.cs</DependentUpon>
    </Compile>
    <Compile Update="MainForm.Designer.cs">
      <DependentUpon>MainForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Data\IssueTracking.cs">
      <DependentUpon>IssueTracking.xsd</DependentUpon>
    </Compile>
    <Compile Update="Data\IssueTracking.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>IssueTracking.xsd</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="GUI\AddIssue.resx">
      <DependentUpon>AddIssue.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="MainForm.resx">
      <DependentUpon>MainForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\IssueTracking.xml" />
    <EmbeddedResource Update="GUI\AssignedIssues.resx">
      <DependentUpon>AssignedIssues.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="GUI\AssignedIssues2.resx">
      <DependentUpon>AssignedIssues2.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="GUI\BatchProcessing.resx">
      <DependentUpon>BatchProcessing.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="GUI\EmployeesIssues.resx">
      <DependentUpon>EmployeesIssues.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="GUI\ReassignFeatures.resx">
      <DependentUpon>ReassignFeatures.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="GUI\ReassignIssue.resx">
      <DependentUpon>ReassignIssue.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Data\IssueTracking.xsc">
      <DependentUpon>IssueTracking.xsd</DependentUpon>
    </None>
    <None Update="Data\IssueTracking.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>IssueTracking.Designer.cs</LastGenOutput>
    </None>
    <None Update="Data\IssueTracking.xss">
      <DependentUpon>IssueTracking.xsd</DependentUpon>
    </None>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
