<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>MESCIUS, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) MESCIUS, Inc. All rights reserved.</Copyright>
    <Authors>MESCIUS, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.C1TrueDBGrid" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.PrintPreview" Version="4.8.20233.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DsComposer.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>DsComposer.xsd</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="DsComposer.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>DsComposer.Designer.cs</LastGenOutput>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="DsComposer.xsc">
      <DependentUpon>DsComposer.xsd</DependentUpon>
    </None>
    <None Update="DsComposer.xss">
      <DependentUpon>DsComposer.xsd</DependentUpon>
    </None>
  </ItemGroup>

</Project>
