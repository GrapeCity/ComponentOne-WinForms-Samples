<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <StartupObject>Tutor15.Form1</StartupObject>
    <MyType>WindowsFormsWithCustomSubMain</MyType>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>MESCIUS, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) MESCIUS, Inc. All rights reserved.</Copyright>
    <Authors>MESCIUS, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Import Include="Microsoft.VisualBasic" />
    <Import Include="System" />
    <Import Include="System.Collections" />
    <Import Include="System.Data" />
    <Import Include="System.Diagnostics" />
    <Import Include="System.Drawing" />
    <Import Include="System.Windows.Forms" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="mscorlib" />
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.C1TrueDBGrid" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.PrintPreview" Version="4.8.20233.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="dsComposer.Designer.vb">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>dsComposer.xsd</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="dsComposer.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>dsComposer.Designer.vb</LastGenOutput>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.vb</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="dsComposer.xsc">
      <DependentUpon>dsComposer.xsd</DependentUpon>
    </None>
    <None Update="dsComposer.xss">
      <DependentUpon>dsComposer.xsd</DependentUpon>
    </None>
    <None Update="dsComposer.xsx">
      <DependentUpon>dsComposer.xsd</DependentUpon>
    </None>
  </ItemGroup>

</Project>
