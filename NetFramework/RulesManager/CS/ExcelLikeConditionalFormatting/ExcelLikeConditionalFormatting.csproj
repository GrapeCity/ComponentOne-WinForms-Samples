<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Drawing.Design" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="UIAutomationProvider" />
    <Reference Include="UIAutomationTypes" />
    <Reference Include="WindowsBase" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.C1Command" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1FlexGrid" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.C1Themes" Version="4.8.20241.*" />
    <PackageReference Include="C1.Win.Ribbon" Version="8.0.20241.*" />
    <PackageReference Include="C1.Win.RulesManager" Version="8.0.20241.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Dialogs\BaseConditionalFormattingForm.Designer.cs">
      <DependentUpon>BaseConditionalFormattingForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\BetweenForm.Designer.cs">
      <DependentUpon>BetweenForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\EqualToForm.Designer.cs">
      <DependentUpon>EqualToForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\GreaterThanForm.Designer.cs">
      <DependentUpon>GreaterThanForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\LessThanForm.Designer.cs">
      <DependentUpon>LessThanForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\SingleValueConditionalFormattingForm.Designer.cs">
      <DependentUpon>SingleValueConditionalFormattingForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\TextThatContainsForm.Designer.cs">
      <DependentUpon>TextThatContainsForm.cs</DependentUpon>
    </Compile>
    <Compile Update="MainForm.Designer.cs">
      <DependentUpon>MainForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Dialogs\BaseConditionalFormattingForm.resx">
      <DependentUpon>BaseConditionalFormattingForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\BetweenForm.resx">
      <DependentUpon>BetweenForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\EqualToForm.resx">
      <DependentUpon>EqualToForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\GreaterThanForm.resx">
      <DependentUpon>GreaterThanForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\LessThanForm.resx">
      <DependentUpon>LessThanForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\SingleValueConditionalFormattingForm.resx">
      <DependentUpon>SingleValueConditionalFormattingForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\TextThatContainsForm.resx">
      <DependentUpon>TextThatContainsForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="MainForm.resx">
      <DependentUpon>MainForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
