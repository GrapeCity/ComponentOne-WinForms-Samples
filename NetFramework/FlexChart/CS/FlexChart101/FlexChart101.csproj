<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexChart" Version="4.8.20232.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\BaseSample.Designer.cs">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\FunnelChart.Designer.cs">
      <DependentUpon>FunnelChart.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\Dynamic.Designer.cs">
      <DependentUpon>Dynamic.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\Toggle.Designer.cs">
      <DependentUpon>Toggle.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\SelectionModes.Designer.cs">
      <DependentUpon>SelectionModes.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\CustomizingAxes.Designer.cs">
      <DependentUpon>CustomizingAxes.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\StylingSeries.Designer.cs">
      <DependentUpon>StylingSeries.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\Tooltips.Designer.cs">
      <DependentUpon>Tooltips.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\MixedChartTypes.Designer.cs">
      <DependentUpon>MixedChartTypes.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\LegendAndTitles.Designer.cs">
      <DependentUpon>LegendAndTitles.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\GettingStarted.Designer.cs">
      <DependentUpon>GettingStarted.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\ChartTypes.Designer.cs">
      <DependentUpon>ChartTypes.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="Localization\ExplorerConfiguration_CH.xml" />
    <Content Include="Localization\ExplorerConfiguration_EN.xml" />
    <Content Include="Localization\ExplorerConfiguration_JP.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\BaseSample.resx">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\FunnelChart.resx">
      <DependentUpon>FunnelChart.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\Dynamic.resx">
      <DependentUpon>Dynamic.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\Toggle.resx">
      <DependentUpon>Toggle.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\SelectionModes.resx">
      <DependentUpon>SelectionModes.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\CustomizingAxes.resx">
      <DependentUpon>CustomizingAxes.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\StylingSeries.resx">
      <DependentUpon>StylingSeries.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\Tooltips.resx">
      <DependentUpon>Tooltips.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\MixedChartTypes.resx">
      <DependentUpon>MixedChartTypes.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\LegendAndTitles.resx">
      <DependentUpon>LegendAndTitles.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\GettingStarted.resx">
      <DependentUpon>GettingStarted.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\ChartTypes.resx">
      <DependentUpon>ChartTypes.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
