<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexChart" Version="8.0.20242.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\BaseSample.Designer.cs">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\LegendRanges.Designer.cs">
      <DependentUpon>LegendRanges.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\LegendItems.Designer.cs">
      <DependentUpon>LegendItems.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\LineStyles.Designer.cs">
      <DependentUpon>LineStyles.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="Localization\ExplorerConfiguration_CH.xml" />
    <Content Include="Localization\ExplorerConfiguration_EN.xml" />
    <Content Include="Localization\ExplorerConfiguration_JP.xml" />
    <Content Include="Resources\apple-50.png" />
    <Content Include="Resources\cb_down.png" />
    <Content Include="Resources\cb_down26.png" />
    <Content Include="Resources\cb_up.png" />
    <Content Include="Resources\cb_up26.png" />
    <Content Include="Resources\huawei-50.png" />
    <Content Include="Resources\lenovo-50.png" />
    <Content Include="Resources\lg-50.png" />
    <Content Include="Resources\oppo-50.png" />
    <Content Include="Resources\samsung-50.png" />
    <Content Include="Resources\vivo-50.png" />
    <Content Include="Resources\xiaomi-50.png" />
    <Content Include="Resources\zte-50.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\BaseSample.resx">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\LegendRanges.resx">
      <DependentUpon>LegendRanges.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\LegendItems.resx">
      <DependentUpon>LegendItems.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\LineStyles.resx">
      <DependentUpon>LineStyles.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
