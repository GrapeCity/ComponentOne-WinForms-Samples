<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>MESCIUS, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) MESCIUS, Inc. All rights reserved.</Copyright>
    <Authors>MESCIUS, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexChart" Version="8.0.20233.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\CustomAnimation.Designer.cs">
      <DependentUpon>CustomAnimation.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\FlexChartAnimation.Designer.cs">
      <DependentUpon>FlexChartAnimation.cs</DependentUpon>
    </Compile>
    <Compile Update="Samples\FlexPieAnimation.Designer.cs">
      <DependentUpon>FlexPieAnimation.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\CustomAnimation.resx">
      <DependentUpon>CustomAnimation.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\FlexChartAnimation.resx">
      <DependentUpon>FlexChartAnimation.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Samples\FlexPieAnimation.resx">
      <DependentUpon>FlexPieAnimation.cs</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
