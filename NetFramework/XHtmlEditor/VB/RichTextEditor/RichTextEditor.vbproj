<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <StartupObject>RichTextEditor.Program</StartupObject>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>MESCIUS inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>©️ MESCIUS inc. All rights reserved.</Copyright>
    <Authors>MESCIUS inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Import Include="Microsoft.VisualBasic" />
    <Import Include="System" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="mscorlib" />
    <Reference Include="System" />
    <Reference Include="System.configuration" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Web.Extensions" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.C1Pdf" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.C1Editor" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.C1InputPanel" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.C1SpellChecker" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.C1SuperTooltip" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.C1Themes" Version="4.8.20233.*" />
    <PackageReference Include="C1.Win.PrintPreview" Version="4.8.20233.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="AppMenuTabs\Items\AppMenuTabButton.Designer.vb">
      <DependentUpon>AppMenuTabButton.vb</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\Items\AppMenuTabFileList.Designer.vb">
      <DependentUpon>AppMenuTabFileList.vb</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\Items\AppMenuTabItem.Designer.vb">
      <DependentUpon>AppMenuTabItem.vb</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\OpenTab.Designer.vb">
      <DependentUpon>OpenTab.vb</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\PrintTab.Designer.vb">
      <DependentUpon>PrintTab.vb</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\SaveAsTab.Designer.vb">
      <DependentUpon>SaveAsTab.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\InsertDateTime.Designer.vb">
      <DependentUpon>InsertDateTime.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\InsertSymbol.Designer.vb">
      <DependentUpon>InsertSymbol.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\SetLanguage.Designer.vb">
      <DependentUpon>SetLanguage.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\WordCount.Designer.vb">
      <DependentUpon>WordCount.vb</DependentUpon>
    </Compile>
    <Compile Update="Form1.Designer.vb">
      <DependentUpon>Form1.vb</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.vb">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.vb">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabButton.resx">
      <DependentUpon>AppMenuTabButton.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabFileList.resx">
      <DependentUpon>AppMenuTabFileList.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabItem.resx">
      <DependentUpon>AppMenuTabItem.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\PrintTab.resx">
      <DependentUpon>PrintTab.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\InsertDateTime.resx">
      <DependentUpon>InsertDateTime.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\InsertSymbol.resx">
      <DependentUpon>InsertSymbol.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\SetLanguage.resx">
      <DependentUpon>SetLanguage.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\WordCount.resx">
      <DependentUpon>WordCount.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <LastGenOutput>Resources.Designer.vb</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <LastGenOutput>Settings.Designer.vb</LastGenOutput>
      <Generator>SettingsSingleFileGenerator</Generator>
    </None>
  </ItemGroup>

</Project>
