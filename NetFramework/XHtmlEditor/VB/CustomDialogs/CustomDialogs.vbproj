<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <StartupObject>CustomDialogs.Program</StartupObject>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Import Include="Microsoft.VisualBasic" />
    <Import Include="System" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="mscorlib" />
    <Reference Include="System" />
    <Reference Include="System.configuration" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Web" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.C1Editor" Version="4.8.20242.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\FindOptions.designer.vb">
      <DependentUpon>FindOptions.vb</DependentUpon>
    </Compile>
    <Compile Update="Controls\ReplaceOptions.designer.vb">
      <DependentUpon>ReplaceOptions.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\BaseEditorForm.designer.vb">
      <DependentUpon>BaseEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\BookmarkEditorForm.designer.vb">
      <DependentUpon>BookmarkEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\CellEditorForm.designer.vb">
      <DependentUpon>CellEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\ColumnEditorForm.designer.vb">
      <DependentUpon>ColumnEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\FindReplaceForm.designer.vb">
      <DependentUpon>FindReplaceForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\FlashMovieEditorForm.designer.vb">
      <DependentUpon>FlashMovieEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\FormattingForm.designer.vb">
      <DependentUpon>FormattingForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\HyperlinkEditorForm.designer.vb">
      <DependentUpon>HyperlinkEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\ImageEditorForm.designer.vb">
      <DependentUpon>ImageEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\RowEditorForm.designer.vb">
      <DependentUpon>RowEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\TableEditorForm.designer.vb">
      <DependentUpon>TableEditorForm.vb</DependentUpon>
    </Compile>
    <Compile Update="Form1.Designer.vb">
      <DependentUpon>Form1.vb</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.vb">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.vb">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\bookmark_16x16.png" />
    <Content Include="Resources\Find.png" />
    <Content Include="Resources\flash_16x16.png" />
    <Content Include="Resources\formatting_16x16.png" />
    <Content Include="Resources\gotoright_16x16.png" />
    <Content Include="Resources\hyperlink_16x16.png" />
    <Content Include="Resources\openfolder_16x16.png" />
    <Content Include="Resources\Picture16.png" />
    <Content Include="Resources\Remove.png" />
    <Content Include="Resources\replace_16x16.png" />
    <Content Include="Resources\table_16x16.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Controls\FindOptions.resx">
      <DependentUpon>FindOptions.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Controls\ReplaceOptions.resx">
      <DependentUpon>ReplaceOptions.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\BaseEditorForm.resx">
      <DependentUpon>BaseEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\BookmarkEditorForm.resx">
      <DependentUpon>BookmarkEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\CellEditorForm.resx">
      <DependentUpon>CellEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\ColumnEditorForm.resx">
      <DependentUpon>ColumnEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\FindReplaceForm.resx">
      <DependentUpon>FindReplaceForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\FlashMovieEditorForm.resx">
      <DependentUpon>FlashMovieEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\FormattingForm.resx">
      <DependentUpon>FormattingForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\HyperlinkEditorForm.resx">
      <DependentUpon>HyperlinkEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\ImageEditorForm.resx">
      <DependentUpon>ImageEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\RowEditorForm.resx">
      <DependentUpon>RowEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\TableEditorForm.resx">
      <DependentUpon>TableEditorForm.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.vb</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <LastGenOutput>Resources.Designer.vb</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <LastGenOutput>Settings.Designer.vb</LastGenOutput>
      <Generator>SettingsSingleFileGenerator</Generator>
    </None>
  </ItemGroup>

</Project>
