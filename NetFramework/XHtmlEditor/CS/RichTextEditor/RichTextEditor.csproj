<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <PlatformTarget>x86</PlatformTarget>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.configuration" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.C1Pdf" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.C1Editor" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.C1InputPanel" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.C1SpellChecker" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.C1SuperTooltip" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.C1Themes" Version="4.8.20232.*" />
    <PackageReference Include="C1.Win.PrintPreview" Version="4.8.20232.*" />
    <PackageReference Include="C1.Zip" Version="2.0.20202.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="AppMenuTabs\Items\AppMenuTabButton.Designer.cs">
      <DependentUpon>AppMenuTabButton.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\Items\AppMenuTabFileList.Designer.cs">
      <DependentUpon>AppMenuTabFileList.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\Items\AppMenuTabItem.Designer.cs">
      <DependentUpon>AppMenuTabItem.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\OpenTab.Designer.cs">
      <DependentUpon>OpenTab.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\PrintTab.Designer.cs">
      <DependentUpon>PrintTab.cs</DependentUpon>
    </Compile>
    <Compile Update="AppMenuTabs\SaveAsTab.Designer.cs">
      <DependentUpon>SaveAsTab.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\InsertDateTime.Designer.cs">
      <DependentUpon>InsertDateTime.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\InsertSymbol.Designer.cs">
      <DependentUpon>InsertSymbol.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\SetLanguage.Designer.cs">
      <DependentUpon>SetLanguage.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\WordCount.Designer.cs">
      <DependentUpon>WordCount.cs</DependentUpon>
    </Compile>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabButton.resx">
      <DependentUpon>AppMenuTabButton.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabFileList.resx">
      <DependentUpon>AppMenuTabFileList.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\Items\AppMenuTabItem.resx">
      <DependentUpon>AppMenuTabItem.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="AppMenuTabs\PrintTab.resx">
      <DependentUpon>PrintTab.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\InsertDateTime.resx">
      <DependentUpon>InsertDateTime.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\InsertSymbol.resx">
      <DependentUpon>InsertSymbol.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\SetLanguage.resx">
      <DependentUpon>SetLanguage.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\WordCount.resx">
      <DependentUpon>WordCount.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
