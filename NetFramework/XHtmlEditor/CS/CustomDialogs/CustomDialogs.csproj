    <Reference Include="System.Deployment" />
    <Reference Include="System.Design" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Web" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.C1Editor" Version="4.8.20232.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Controls\FindOptions.designer.cs">
      <DependentUpon>FindOptions.cs</DependentUpon>
    </Compile>
    <Compile Update="Controls\ReplaceOptions.designer.cs">
      <DependentUpon>ReplaceOptions.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\BaseEditorForm.designer.cs">
      <DependentUpon>BaseEditorForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\BookmarkEditorForm.designer.cs">
      <DependentUpon>BookmarkEditorForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\CellEditorForm.designer.cs">
      <DependentUpon>CellEditorForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\ColumnEditorForm.designer.cs">
      <DependentUpon>ColumnEditorForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\FindReplaceForm.designer.cs">
      <DependentUpon>FindReplaceForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\FormattingForm.designer.cs">
      <DependentUpon>FormattingForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\HyperlinkEditorForm.designer.cs">
      <DependentUpon>HyperlinkEditorForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Dialogs\ImageEditorForm.designer.cs">
