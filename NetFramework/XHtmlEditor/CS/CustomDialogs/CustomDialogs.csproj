    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\ImageEditorForm.resx">
      <DependentUpon>ImageEditorForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\FlashMovieEditorForm.resx">
      <DependentUpon>FlashMovieEditorForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dialogs\RowEditorForm.resx">
