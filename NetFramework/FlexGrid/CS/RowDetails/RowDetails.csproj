<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net48</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<PlatformTarget>x86</PlatformTarget>
		<ApplicationIcon>C1.ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
		<Reference Include="Microsoft.CSharp" />
		<Reference Include="System" />
		<Reference Include="System.Core" />
		<Reference Include="System.Data" />
		<Reference Include="System.Data.DataSetExtensions" />
		<Reference Include="System.Drawing" />
		<Reference Include="System.Windows.Forms" />
		<Reference Include="System.Xml" />
		<Reference Include="System.Xml.Linq" />
		<Reference Include="UIAutomationClient" />
		<Reference Include="UIAutomationClientsideProviders" />
		<Reference Include="UIAutomationProvider" />
		<Reference Include="UIAutomationTypes" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="C1.Win.Command" Version="8.0.20252.*" />
		<PackageReference Include="C1.Win.FlexGrid" Version="8.0.20252.*" />
		<PackageReference Include="C1.Win.FlexGrid.RowDetails" Version="8.0.20252.*" />
		<PackageReference Include="C1.Win.C1Input" Version="4.8.20252.*" />
		<PackageReference Include="C1.Win.InputPanel" Version="8.0.20252.*" />
