		</Compile>
		<Compile Update="Samples\InputPanelSample.Designer.cs">
			<DependentUpon>InputPanelSample.cs</DependentUpon>
		</Compile>
		<Compile Update="Form1.Designer.cs">
			<DependentUpon>Form1.cs</DependentUpon>
		</Compile>
	</ItemGroup>

