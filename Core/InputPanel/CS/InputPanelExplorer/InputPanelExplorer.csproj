    </EmbeddedResource>
    <EmbeddedResource Include="Samples\DataBinding.resx" />
    <EmbeddedResource Include="Samples\Customization.resx" />
    <EmbeddedResource Include="Samples\BillOfSale.resx" />
    <EmbeddedResource Include="Samples\FlowPanelСatalogue.resx" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    
    
	<PackageReference Include="C1.Win.InputPanel" Version="[5.0.20212.501,)" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="3.1.9" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="3.1.9" />
