    <Compile Include="Program.cs" />
    <Compile Include="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
<Compile Include="Resource1.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource1.resx</DependentUpon>
    </Compile>
    <Compile Include="Samples\DataBinding.cs" />
    <Compile Include="Samples\DataBinding.Designer.cs" />
    <Compile Include="Samples\Customization.cs" />
    <Compile Include="Samples\Customization.Designer.cs" />
<Compile Include="Samples\BillOfSale.cs" />
    <Compile Include="Samples\BillOfSale.Designer.cs" />
    <Compile Include="Samples\FlowPanelСatalogue.cs" />
    <Compile Include="Samples\FlowPanelСatalogue.Designer.cs" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="readme.txt" />
<None Include="Resources\rolls-roys-Wraith.jpg" />
    <None Include="Resources\Ford_Ranger.jpg" />
    <None Include="Resources\Jaguar-F-type1.jpg" />
    <None Include="Resources\Mercedes-Benz_SLK_250.jpg" />
    <None Include="Resources\lexus ES VI Sedan.jpg" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Form1.resx" />
<EmbeddedResource Include="Resource1.resx">
