<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.Pdf" Version="10.0.20252.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\borders.htm" />
    <EmbeddedResource Include="Resources\fonts.htm" />
    <EmbeddedResource Include="Resources\images.htm" />
    <EmbeddedResource Include="Resources\indent.htm" />
    <EmbeddedResource Include="Resources\Lawler.htm" />
    <EmbeddedResource Include="Resources\lists.htm" />
    <EmbeddedResource Include="Resources\longdoc.htm" />
    <EmbeddedResource Include="Resources\Montgomery.htm" />
    <EmbeddedResource Include="Resources\nesting.htm" />
    <EmbeddedResource Include="Resources\padding.htm" />
    <EmbeddedResource Include="Resources\paragraph.htm" />
    <EmbeddedResource Include="Resources\RexSwain.htm" />
    <EmbeddedResource Include="Resources\stylesheet.htm" />
    <EmbeddedResource Include="Resources\tables.htm" />
    <EmbeddedResource Include="Resources\tableSizing.htm" />
    <EmbeddedResource Include="Resources\w3c.htm" />
    <EmbeddedResource Include="Resources\whitespace.htm" />
    <EmbeddedResource Include="Resources\wordwrap.htm" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
