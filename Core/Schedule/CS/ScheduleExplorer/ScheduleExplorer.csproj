<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
          </PropertyGroup>

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
	<RuntimeIdentifier>win-x86</RuntimeIdentifier>
    <UseWindowsForms>true</UseWindowsForms>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultNoneItems>false</EnableDefaultNoneItems>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Data\DataSource.cs" />
    <Compile Include="Data\Samples.cs" />
    <Compile Include="Form1.cs" />
    <Compile Include="Form1.Designer.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Include="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
    <Compile Include="Samples\C1ScheduleDemo.cs" />
    <Compile Include="Samples\C1ScheduleDemo.Designer.cs" />
    <Compile Include="Samples\UnBound.cs" />
    <Compile Include="Samples\UnBound.Designer.cs" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="C1Schedule.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Form1.resx" />
    <EmbeddedResource Include="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Samples\C1ScheduleDemo.resx" />
    <EmbeddedResource Include="Samples\UnBound.resx" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.Internal.License" Version="5.20203.1" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="3.1.9" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="3.1.9" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\C1.Win.Barcode\C1.Win.Barcode.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\C1.Win\C1.Win.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\PrintDocument\C1Preview\C1.PrintDocument.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\PrintDocument\Win.C1Preview\C1.Win.PrintPreview.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\PrintDocument\Win.C1RibbonPreview\C1.Win.RibbonPreview.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\Ribbon\C1.Win.Ribbon.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\Schedule\C1.Win.Schedule.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\SplitContainer\C1.Win.SplitContainer.5.csproj" />
    <ProjectReference Include="..\..\..\..\..\c1-winforms\Controls\SuperTooltip\C1.Win.SuperTooltip.5.csproj" />
  </ItemGroup>

</Project>