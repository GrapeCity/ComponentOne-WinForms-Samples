<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove=".gclicx" />
    <None Remove="Resources\home.rtf" />
    <None Remove="Resources\iconFeatures.png" />
    <None Remove="Resources\iconIntro.png" />
    <None Remove="Resources\iconWhatsNew.png" />
  </ItemGroup> 

  <ItemGroup>
    <Content Include="Resources\C1Samples.xml" />

	  <Content Include="Resources\C1Samples.xml">
		  <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
  </ItemGroup>

  <ItemGroup>
	  <PackageReference Include="C1.Win.Input" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.FlexGrid" Version="10.0.20252.*" />
	  <PackageReference Include="Microsoft.Data.Sqlite" Version="9.0.5" />
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	  <PackageReference Include="C1.Win.Input.DbNavigator" Version="10.0.20252.*" />

	  <PackageReference Include="C1.DataCollection.BindingList" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Command" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.ExpressionEditor" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.FlexChart" Version="10.0.20252.*" /> 
	  <PackageReference Include="C1.Win.FlexGrid.BandedView" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.FlexGrid.GroupPanel" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.FlexGrid.ImportExport" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.FlexGrid.RowDetails" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Input" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Input.Pickers" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.InputPanel" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.List" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Pdf" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Ribbon" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.SuperTooltip" Version="10.0.20252.*" />
	  <PackageReference Include="C1.Win.Themes" Version="10.0.20252.*" />
	  <PackageReference Include="CsvHelper" Version="33.1.0" />
	  <PackageReference Include="Microsoft.Data.Sqlite" Version="9.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\home.rtf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include=".gclicx" />
    <EmbeddedResource Include="Resources\iconFeatures.png" />
    <EmbeddedResource Include="Resources\iconIntro.png" />
    <EmbeddedResource Include="Resources\iconWhatsNew.png" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Samples\TransposedGrid\TransposedGrid.sln" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\StringsEN.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>StringsEN.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\StringsEN.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>StringsEN.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Samples\TransposedGrid\Data\hotel_dataset.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>