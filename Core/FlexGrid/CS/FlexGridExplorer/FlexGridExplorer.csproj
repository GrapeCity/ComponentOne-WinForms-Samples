<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.FlexGrid" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.FlexGrid.BandedView" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.FlexGrid.GroupPanel" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.FlexGrid.ImportExport" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.FlexGrid.RowDetails" Version="6.0.20231.*" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="7.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
