<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Globals">
          </PropertyGroup>
  
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
	<RuntimeIdentifier>win-x86</RuntimeIdentifier>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>App.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultNoneItems>false</EnableDefaultNoneItems>
    <Authors>GrapeCity, Inc.</Authors>
    <Product>ComponentOne WinForms Edition</Product>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="App.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="App.ico" />
    <EmbeddedResource Include="MainForm.resx" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.FlexGrid" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.FlexGrid.ImportExport" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.Ribbon" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.RulesManager" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.SuperTooltip" Version="[5.0.20211.489,)" />
    <PackageReference Include="C1.Win.Themes" Version="[5.0.20211.489,)" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="CustomRowDetail.cs" />
    <Compile Include="MainForm.cs" />
    <Compile Include="MainForm.Designer.cs" />
    <Compile Include="Program.cs" />
    <EmbeddedResource Include="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <SubType>Designer</SubType>
    </EmbeddedResource>
    <Compile Include="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="Greenwich.c1themez">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <None Include="readme.txt" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Resources\" />
  </ItemGroup>

</Project>