    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="C1XapOptimizer.pdf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

