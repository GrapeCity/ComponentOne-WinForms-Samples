<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net10.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexReport" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.FlexViewer" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Input.MultiSelect" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Ribbon" Version="10.0.20252.*" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include=".gclicx" />
  </ItemGroup>

  <ItemGroup>
    <None Update="FlexReportDemo.flxr">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
