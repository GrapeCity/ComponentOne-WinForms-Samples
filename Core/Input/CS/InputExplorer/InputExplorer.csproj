<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>Mescius, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) Mescius, Inc. All rights reserved.</Copyright>
    <Authors>Mescius, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.Calendar" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Input" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Input.DbNavigator" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Input.Pickers" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Themes" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.TreeView" Version="6.0.20232.*" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="7.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
