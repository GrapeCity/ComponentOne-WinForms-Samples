<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <DefineConstants>WinForms</DefineConstants>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>MESCIUS inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>©️ MESCIUS inc. All rights reserved.</Copyright>
    <Authors>MESCIUS inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.BarCode" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Command" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.DashboardLayout" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.DataFilter" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Document" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.DX" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexChart" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexGrid" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexGrid.GroupPanel" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexPivot" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexReport" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.FlexViewer" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Gauge" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Input" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Map" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Ribbon" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.SplitContainer" Version="8.0.20233.*" />
    <PackageReference Include="C1.Win.Themes" Version="8.0.20233.*" />
    <PackageReference Include="C1.Zip" Version="2.0.20202.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="7.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Analysis.Designer.cs">
      <DependentUpon>Analysis.cs</DependentUpon>
    </Compile>
    <Compile Update="BulletGraphs.Designer.cs">
      <DependentUpon>BulletGraphs.cs</DependentUpon>
    </Compile>
    <Compile Update="Dashboard.Designer.cs">
      <DependentUpon>Dashboard.cs</DependentUpon>
    </Compile>
    <Compile Update="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Update="Gauges.Designer.cs">
      <DependentUpon>Gauges.cs</DependentUpon>
    </Compile>
    <Compile Update="Products.Designer.cs">
      <DependentUpon>Products.cs</DependentUpon>
    </Compile>
    <Compile Update="RangeSelector.Designer.cs">
      <DependentUpon>RangeSelector.cs</DependentUpon>
    </Compile>
    <Compile Update="Reporting.Designer.cs">
      <DependentUpon>Reporting.cs</DependentUpon>
    </Compile>
    <Compile Update="Strings\Resource.Designer.cs">
      <DependentUpon>Resource.resx</DependentUpon>
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Tasks.Designer.cs">
      <DependentUpon>Tasks.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Analysis.resx">
      <DependentUpon>Analysis.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Dashboard.resx">
      <DependentUpon>Dashboard.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Form1.resx">
      <DependentUpon>Form1.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Gauges.resx">
      <DependentUpon>Gauges.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Products.resx">
      <DependentUpon>Products.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="RangeSelector.resx">
      <DependentUpon>RangeSelector.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Reporting.resx">
      <DependentUpon>Reporting.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Strings\Resource.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Tasks.resx">
      <DependentUpon>Tasks.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Include="Reports\CurrentOpportunitiesData.flxr" />
    <EmbeddedResource Include="Reports\ProfitAndSales.flxr" />
    <EmbeddedResource Include="Reports\RegionWiseSalesData.flxr" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

  <Import Project="..\Model\Model.projitems" Label="Shared" />

</Project>
