    <PackageReference Include="C1.Zip" Version="2.0.20202.1" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="7.0.2" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="Analysis.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Include="Analysis.Designer.cs">
      <DependentUpon>Analysis.cs</DependentUpon>
    </Compile>
    <Compile Include="BulletGraphs.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Include="BulletGraphs.Designer.cs">
      <DependentUpon>BulletGraphs.cs</DependentUpon>
    </Compile>
    <Compile Include="Dashboard.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Include="Dashboard.Designer.cs">
      <DependentUpon>Dashboard.cs</DependentUpon>
    </Compile>
    <Compile Include="Form1.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Include="Form1.Designer.cs">
      <DependentUpon>Form1.cs</DependentUpon>
    </Compile>
    <Compile Include="Gauges.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Include="Gauges.Designer.cs">
      <DependentUpon>Gauges.cs</DependentUpon>
    </Compile>
    <Compile Include="Products.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Include="Products.Designer.cs">
      <DependentUpon>Products.cs</DependentUpon>
    </Compile>
