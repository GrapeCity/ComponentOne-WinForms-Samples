    <PackageReference Include="C1.Win.Gauge" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.Input" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.SplitContainer" Version="6.0.20231.*" />
    <PackageReference Include="C1.Win.Themes" Version="6.0.20231.*" />
