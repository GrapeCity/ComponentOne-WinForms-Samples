<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RuntimeIdentifier>win-x86</RuntimeIdentifier>
    <UseWindowsForms>true</UseWindowsForms>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <EnableDefaultEmbeddedResourceItems>false</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultNoneItems>false</EnableDefaultNoneItems>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Data\DataService.cs" />
    <Compile Include="Data\DataSource.cs" />
    <Compile Include="Data\Enums.cs" />
    <Compile Include="Data\Quote.cs" />
    <Compile Include="Data\Samples.cs" />
    <Compile Include="Data\TransmissAutomatic.cs" />
    <Compile Include="Extensions\AggregateExtension.cs" />
    <Compile Include="Extensions\DateTimeExtension.cs" />
    <Compile Include="MainForm.cs" />
    <Compile Include="MainForm.Designer.cs" />
    <Compile Include="Program.cs" />
    <Compile Include="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Include="Samples\ChartAsDataFilter.cs" />
    <Compile Include="Samples\ChartAsDataFilter.Designer.cs" />
    <Compile Include="Samples\Controls\DropDownFilter.cs" />
    <Compile Include="Samples\FilterSummary.cs" />
    <Compile Include="Samples\FilterSummary.Designer.cs" />
    <Compile Include="Samples\Filters\FlexChartDateTimeFilter.cs" />
    <Compile Include="Samples\Filters\FlexChartFilter.cs" />
    <Compile Include="Samples\Controls\FlexChartDateTimeFilterView.cs" />
    <Compile Include="Samples\Controls\FlexChartDateTimeFilterView.Designer.cs" />
    <Compile Include="Samples\Controls\FlexChartFilterView.cs" />
    <Compile Include="Samples\Controls\FlexChartFilterView.Designer.cs" />
    <Compile Include="Samples\Overview.cs" />
    <Compile Include="Samples\Overview.Designer.cs" />
    <Compile Include="Samples\Controls\RangeSliderView.cs" />
    <Compile Include="Samples\Controls\RangeSliderView.Designer.cs" />
    <Compile Include="Samples\UnboundMode.cs" />
    <Compile Include="Samples\UnboundMode.Designer.cs" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="readme.txt" />
    <None Include="Resources\HistoricalQuotes.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <Content Include="screenshot.PNG" />
    <EmbeddedResource Include="MainForm.resx" />
    <EmbeddedResource Include="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Samples\ChartAsDataFilter.resx" />
    <EmbeddedResource Include="Samples\FilterSummary.resx" />
    <EmbeddedResource Include="Samples\Controls\FlexChartDateTimeFilterView.resx" />
    <EmbeddedResource Include="Samples\Controls\FlexChartFilterView.resx" />
    <EmbeddedResource Include="Samples\Overview.resx" />
    <EmbeddedResource Include="Samples\Controls\RangeSliderView.resx" />
    <EmbeddedResource Include="Samples\UnboundMode.resx" />
  </ItemGroup>

  <ItemGroup>
    <None Include="README.md" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.Input" Version="5.0.20212.*" />
    <PackageReference Include="C1.Win.Themes" Version="5.0.20212.*" />    
    <PackageReference Include="C1.Win.DataFilter" Version="5.0.20212.*" />
    <PackageReference Include="C1.Win.FlexGrid" Version="5.0.20212.*" />
    <PackageReference Include="C1.Win.FlexChart" Version="5.0.20212.*" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="3.1.9" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="3.1.9" />
  </ItemGroup>

</Project>