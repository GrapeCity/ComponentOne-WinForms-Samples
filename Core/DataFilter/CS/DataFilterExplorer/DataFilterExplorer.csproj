<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.DataEngine.Api" Version="8.0.20242.340" />
    <PackageReference Include="C1.DataEngine.Collection" Version="8.0.20242.340" />
    <PackageReference Include="C1.Win.Command" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.DataFilter" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.FlexChart" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.FlexGrid" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.Input" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.List" Version="8.0.20242.*" />
    <PackageReference Include="C1.Win.Themes" Version="8.0.20242.*" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="8.0.10" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\AdvancedFilter.png" />
    <EmbeddedResource Include="Resources\AdvancedFilterHot.png" />
    <EmbeddedResource Include="Resources\Filter.png" />
    <EmbeddedResource Include="Resources\FilterClose.png" />
    <EmbeddedResource Include="Resources\FilterHot.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\HistoricalQuotes.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
