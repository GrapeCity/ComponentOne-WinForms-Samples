<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>C1.ico</ApplicationIcon>
    <Company>GrapeCity, Inc.</Company>
    <Product>ComponentOne WinForms Edition</Product>
    <Copyright>(c) GrapeCity, Inc. All rights reserved.</Copyright>
    <Authors>GrapeCity, Inc.</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.DataEngine.Api" Version="5.0.20231.303" />
    <PackageReference Include="C1.DataEngine.Collection" Version="5.0.20231.303" />
    <PackageReference Include="C1.Win.Command" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.DataFilter" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.FlexChart" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.FlexGrid" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Input" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.List" Version="6.0.20232.*" />
    <PackageReference Include="C1.Win.Themes" Version="6.0.20232.*" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="7.0.2" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Include="Resources\AdvancedFilter.png" />
    <EmbeddedResource Include="Resources\AdvancedFilterHot.png" />
    <EmbeddedResource Include="Resources\Filter.png" />
    <EmbeddedResource Include="Resources\FilterClose.png" />
    <EmbeddedResource Include="Resources\FilterHot.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\HistoricalQuotes.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
