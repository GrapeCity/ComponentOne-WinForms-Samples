<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>icon_flexchartexplorer.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="Components\BaseSample.Designer.cs">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\DescriptionPanel.Designer.cs">
      <DependentUpon>DescriptionPanel.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\Header.Designer.cs">
      <DependentUpon>Header.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\StackedTree\StackedTreeControl.Designer.cs">
      <DependentUpon>StackedTreeControl.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\SampleHost.designer.cs">
      <DependentUpon>SampleHost.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\StackedTree\StackNodeControl.designer.cs">
      <DependentUpon>StackNodeControl.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\TileControl\TileControl.Designer.cs">
      <DependentUpon>TileControl.cs</DependentUpon>
    </Compile>
    <Compile Update="Components\TileControl\TileView.Designer.cs">
      <DependentUpon>TileView.cs</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Content Include="icon_flexchartexplorer.ico" />
    <Content Include="Resources\C1.png" />
    <Content Include="Resources\C1_Hex.png" />
    <Content Include="Resources\C1_WaterMark.png" />
    <Content Include="Resources\icon_cancel.png" />
    <Content Include="Resources\icon_Home.png" />
    <Content Include="Resources\icon_Intro.png" />
    <Content Include="Resources\icon_leaf.png" />
    <Content Include="Resources\icon_Minus.png" />
    <Content Include="Resources\icon_Plus.png" />
    <Content Include="Resources\icon_search.png" />
    <Content Include="Resources\icon_Triangle_Closed.png" />
    <Content Include="Resources\icon_Triangle_Open.png" />
    <Content Include="Resources\Img_Info.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.Command" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Input" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.SuperTooltip" Version="10.0.20252.*" />
    <PackageReference Include="C1.Win.Themes" Version="10.0.20252.*" />
    <PackageReference Include="System.Text.Json" Version="9.0.5" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Components\BaseSample.resx">
      <DependentUpon>BaseSample.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\DescriptionPanel.resx">
      <DependentUpon>DescriptionPanel.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\Header.resx">
      <DependentUpon>Header.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\StackedTree\StackedTreeControl.resx">
      <DependentUpon>StackedTreeControl.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\SampleHost.resx">
      <DependentUpon>SampleHost.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\StackedTree\StackNodeControl.resx">
      <DependentUpon>StackNodeControl.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\TileControl\TileControl.resx">
      <DependentUpon>TileControl.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Components\TileControl\TileView.resx">
      <DependentUpon>TileView.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
