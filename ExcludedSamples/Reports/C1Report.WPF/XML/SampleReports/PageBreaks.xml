  <!--Report *** Products By Row ***-->
  <Report version="2.5.20061.195">
    <Name>Products By Row</Name>
    <ReportInfo />
    <DataSource>
      <ConnectionString>Provider=Microsoft.Jet.OLEDB.4.0;Data Source=C:\Program Files\ComponentOne Studio.NET 2.0\common\c1nwind.mdb;Persist Security Info=False</ConnectionString>
      <RecordSource>PARAMETERS GroupSize short 5,
                         ColumnBreaks boolean false, 
                         PageBreaks boolean false, 
                         Across boolean false;
SELECT DISTINCTROW Categories.CategoryName, Products.ProductName, Products.QuantityPerUnit, Products.UnitsInStock, Products.Discontinued, Categories.Picture
        FROM Categories INNER JOIN Products ON Categories.CategoryID = Products.CategoryID
        WHERE (((Products.Discontinued)&lt;&gt;Yes))
        ORDER BY Categories.CategoryName, Products.ProductName;</RecordSource>
    </DataSource>
    <Layout>
      <Width>2520</Width>
      <MarginLeft>1080</MarginLeft>
      <MarginRight>720</MarginRight>
      <Columns>4</Columns>
      <ColumnLayout>1</ColumnLayout>
    </Layout>
    <Font>
      <Name>Arial</Name>
      <Size>9</Size>
    </Font>
    <OnOpen>Report.Layout.ColumnLayout = 0  'down
if Across then Report.Layout.ColumnLayout = 1 'across</OnOpen>
    <Groups>
      <Group>
