<Window x:Class="CustomFields_2010.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Custom Fields" Height="350" Width="525" xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml">
    <Grid>
        <c1:C1DocumentViewer Name="c1DocumentViewer1"  Margin="200,0,0,20" />
        <Grid Name="gridReports" Width="200" HorizontalAlignment="Left" >
            <Label Name="labelSelectReport" Content="Select a Report:" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" FontSize="14" Width="180" Height="30" FontWeight="Bold"/>
        </Grid>
        <Label Name="labelStatus" Content="Ready" HorizontalAlignment="Left" Margin="200,0,0,0" VerticalAlignment="Bottom" RenderTransformOrigin="2.575,-3.15"/>
    </Grid>
</Window>
